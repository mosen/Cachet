{
    "name": "cachethq/cachet",
    "description": "An open source status page system, for everyone.",
    "keywords": [
        "laravel",
        "cachet",
        "alt-three",
        "status",
        "page"
    ],
    "type": "project",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "James Brooks",
            "email": "james@alt-three.com"
        },
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        },
        {
            "name": "Joseph Cohen",
            "email": "joe@alt-three.com"
        }
    ],
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-mbstring": "*"
    },
    "require": {
        "php": "^7.2.5",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-xml": "*",
        "alt-three/bus": "^4.4",
        "alt-three/validator": "^4.4",
        "aws/aws-sdk-php": "^3.133.46",
        "chillerlan/php-qrcode": "^3.2.1",
        "doctrine/dbal": "2.9.*",
        "fideloper/proxy": "^4.3",
        "fruitcake/laravel-cors": "^1.0",
        "graham-campbell/binput": "^7.1.0",
        "graham-campbell/exceptions": "^14.0",
        "graham-campbell/markdown": "^12.0",
        "guzzlehttp/guzzle": "^6.5",
        "jenssegers/date": "4.0.0-beta",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "mccool/laravel-auto-presenter": "^7.4",
        "nexmo/client": "^2.0",
        "pragmarx/google2fa": "^7.0",
        "predis/predis": "^1.1",
        "twig/twig": "^2.12.5"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "ext-sqlite3": "*",
        "alt-three/testbench": "^4.1",
        "barryvdh/laravel-debugbar": "^3.2",
        "bugsnag/bugsnag-laravel": "^2.15",
        "filp/whoops": "^2.7",
        "fzaninotto/faker": "^1.9.1",
        "graham-campbell/analyzer": "^3.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "ejunker/mailthief": "^0.3.16"
    },
    "suggest": {
        "ext-apc": "APC Support cache driver."
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "CachetHQ\\Cachet\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CachetHQ\\Tests\\Cachet\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
          "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
      "post-create-project-cmd": [
        "@php artisan key:generate --ansi"
      ],
      "post-autoload-dump": [
        "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
        "@php artisan package:discover --ansi"
      ]
    },
    "config": {
        "platform": {
            "php": "7.3.9"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
